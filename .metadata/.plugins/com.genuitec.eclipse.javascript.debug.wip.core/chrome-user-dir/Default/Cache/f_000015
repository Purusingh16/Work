var SELECT_VALUE_ACCESSOR = {
    provide: NG_VALUE_ACCESSOR,
    useExisting: __webpack_require__.i(__WEBPACK_IMPORTED_MODULE_0__angular_core__["Y" /* forwardRef */])(function () { return SelectControlValueAccessor; }),
    multi: true
